{
  "name": "manim-js",
  "version": "0.1.0",
  "description": "Manim-like mathematical animation library for the web",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/integrations/react.js",
      "types": "./dist/integrations/react.d.ts"
    },
    "./vue": {
      "import": "./dist/integrations/vue.js",
      "types": "./dist/integrations/vue.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "earcut": "^3.0.2",
    "gif.js": "^0.2.0",
    "katex": "^0.16.28",
    "opentype.js": "^1.3.4",
    "polygon-clipping": "^0.15.7",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/earcut": "^3.0.0",
    "@types/katex": "^0.16.8",
    "@types/react": "^18.0.0",
    "@types/three": "^0.160.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "keywords": [
    "animation",
    "math",
    "visualization",
    "manim",
    "threejs",
    "webgl"
  ],
  "license": "MIT"
}
